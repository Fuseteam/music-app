{
    "policy_version": 1.2,
    "policy_groups": [
        "audio",
        "music_files_read",
        "networking",
        "usermetrics"
    ],
    "read_path": [
      "/etc/fstab",
      "/proc/[0-9]*/mounts",
      "@{HOME}/.cache/media-art/",
      "@{HOME}/.cache/mediascanner-2.0/"
    ]
}
